    fire1:Vec3; // 阵营1火站位 
    light1:Vec3; // 阵营1光站位 
    wind1:Vec3; // 阵营1风站位 
    water1:Vec3; // 阵营1水站位 
    hero2:Vec3; // 阵营2英雄站位 
    fire2:Vec3; // 阵营2火站位 
